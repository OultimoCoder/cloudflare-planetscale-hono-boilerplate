{
  "name": "cf-hono-app",
  "version": "0.0.1",
  "description": "Create a Cloudflare workers app for building production ready RESTful APIs using Hono",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "rimraf dist && node ./build.js",
    "dev": "miniflare --live-reload --debug --env .env",
    "migrate:dev:latest": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts latest",
    "migrate:dev:down": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts down"
  },
  "type": "module",
  "author": "Ben Armstrong <ben.armstrong22@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.11",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "miniflare": "^2.11.0",
    "ts-node": "^10.9.1",
    "wrangler": "^2.1.13"
  },
  "dependencies": {
    "@planetscale/database": "^1.4.0",
    "@tsndr/cloudflare-worker-jwt": "2.1.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.6",
    "hono": "^2.2.5",
    "http-status": "^1.5.3",
    "kysely": "^0.22.0",
    "kysely-planetscale": "^1.1.0",
    "zod": "^3.19.1"
  }
}
