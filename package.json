{
  "name": "create-cf-planetscale-app",
  "version": "1.1.0",
  "description": "Create a Cloudflare workers app for building production ready RESTful APIs using Hono",
  "main": "dist/index.mjs",
  "engines": {
    "node": ">=12.0.0"
  },
  "bin": "bin/createApp.js",
  "repository": "https://github.com/OultimoCoder/cloudflare-planetscale-hono-boilerplate.git",
  "author": "Ben Louis Armstrong <ben.armstrong22@gmail.com>",
  "license": "MIT",
  "keywords": [
    "cloudflare",
    "workers",
    "cloudflare-worker",
    "cloudflare-workers",
    "planetscale",
    "boilerplate",
    "template",
    "starter",
    "example",
    "jest",
    "hono",
    "api",
    "rest",
    "sql",
    "oauth",
    "jwt",
    "es6",
    "es7",
    "es8",
    "es9",
    "jwt",
    "zod",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "build": "node ./build.js",
    "dev": "miniflare --live-reload --debug --env .env --modules dist/index.mjs",
    "tests": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles --runInBand",
    "tests:coverage": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles --runInBand --coverage",
    "tests:summary": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles --runInBand --coverage --coverageReporters=\"text-summary\"",
    "tests:unit": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles ./tests/unit",
    "tests:integration": "npm run build && cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles ./tests/integration",
    "migrate:dev:latest": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts dev latest",
    "migrate:dev:down": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts dev down",
    "migrate:dev:none": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts dev none",
    "migrate:test:latest": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test latest",
    "migrate:test:none": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test none",
    "migrate:test:down": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test down",
    "migrate:prod:latest": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test latest",
    "migrate:prod:none": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test none",
    "migrate:prod:down": "node --experimental-specifier-resolution=node --loader ts-node/esm ./scripts/migrate.ts test down",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/**/*.ts",
    "prepare": "husky install",
    "deploy:dev": "wrangler publish",
    "deploy:prod": "wrangler publish --env production"
  },
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230724.0",
    "@faker-js/faker": "^8.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.17",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-miniflare": "^2.14.0",
    "miniflare": "^3.20230724.0",
    "mockdate": "^3.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wrangler": "^3.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.377.0",
    "@hono/sentry": "^0.2.1",
    "@planetscale/database": "^1.10.0",
    "@tsndr/cloudflare-worker-jwt": "2.2.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.9",
    "hono": "^3.3.3",
    "http-status": "^1.6.2",
    "kysely": "^0.26.1",
    "kysely-planetscale": "^1.3",
    "toucan-js": "3.1.0",
    "worker-auth-providers-typed": "^0.0.10-beta7-7",
    "zod": "^3.21.4",
    "zod-error": "^1.5.0"
  }
}
